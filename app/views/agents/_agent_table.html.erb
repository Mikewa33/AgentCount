<table id="user-agent-table">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>
      <th>User Agent</th>
      <th>Visit Count</th>
      <th>First Visit</th>
      <th>Last Visit</th>
    </tr>
    <% agents.sort { |a, b| b.counter <=> a.counter }.each do |agent| %>
      <tr>
        <td><%= agent.name %></td>
        <td><%= agent.counter %></td>
        <td><%= agent.created_at %></td>
        <td><%= agent.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
